{
  "Version": "2012-10-17",
  "Statement": [
    { "Effect": "Allow", "Action": [
        "redshift-data:ExecuteStatement",
        "redshift-data:DescribeStatement",
        "redshift-data:GetStatementResult"
      ], "Resource": "*" },
    { "Effect": "Allow", "Action": "secretsmanager:GetSecretValue", "Resource": "arn:aws:secretsmanager:us-east-1:ACCOUNT_ID:secret:redshift/esc" }
  ]
}
